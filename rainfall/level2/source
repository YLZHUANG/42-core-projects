
char *p(void)
{
    char s[64];
    const void *v2;
    unsigned int retaddr;

    fflush(stdout);
    gets(s);
    v2 = (const void *)retaddr;
    if ( (retaddr & 0xb0000000) == 0xb0000000 )
    {
        printf("(%p)\n", v2);
        exit(1);
    }
    puts(s);
    return strdup(s);
}

int main(void)
{
    return p();
}